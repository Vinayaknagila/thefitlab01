<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/youcann"
    tools:context=".MainActivity3">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="403dp"
        android:layout_height="212dp"
        android:background="@drawable/top_background"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="223dp"
        android:layout_height="123dp"
        app:cardCornerRadius="21dp"
        app:cardElevation="21dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cardView3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/card_background"
            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="20dp"
                android:scaleType="centerCrop"
                android:src="@drawable/logo1">

            </ImageView>
        </LinearLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="352dp"
        android:layout_height="499dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.698">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="149dp"
            android:layout_height="48dp"
            android:layoutDirection="inherit"
            android:layout_marginTop="230dp"
            android:text="BellyFat"
            android:textAlignment="center"
            android:textSize="28dp">

        </TextView>
        <TextView
            android:id="@+id/textView4"
            android:layout_width="149dp"
            android:layout_height="48dp"
            android:layoutDirection="inherit"
            android:layout_marginTop="430dp"
            android:text="Yoga"
            android:textAlignment="center"
            android:textSize="28dp">>

        </TextView>
        <TextView
            android:id="@+id/textView5"
            android:layout_width="149dp"
            android:layout_height="48dp"
            android:layoutDirection="inherit"
            android:layout_gravity="right|center_horizontal"
            android:layout_marginTop="230dp"
            android:text="FullBody"
            android:textAlignment="center"
            android:textSize="28dp">>

        </TextView>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="146dp"
            android:layout_height="59dp"
            android:layout_gravity="right|center_horizontal"
            android:layout_marginTop="430dp"
            android:layoutDirection="inherit"
            android:text="Resistance Band"
            android:textAlignment="center"
            android:textSize="26dp">

        </TextView>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/waterbutton1"
        android:layout_width="123dp"
        android:layout_height="127dp"
        android:layout_gravity="left"
        android:visibility="invisible"
        android:layout_marginTop="302dp"
        android:layout_marginEnd="216dp"
        app:cardCornerRadius="21dp"
        app:cardElevation="21dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/card_background"
            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dp"
                android:scaleType="centerCrop"
                android:src="@drawable/yoga">

            </ImageView>
        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/waterbutton15"
        android:layout_width="123dp"
        android:layout_height="127dp"
        android:layout_gravity="left"
        android:visibility="invisible"
        android:layout_marginStart="216dp"
        android:layout_marginTop="302dp"
        app:cardCornerRadius="21dp"
        app:cardElevation="21dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/card_background"
            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dp"
                android:scaleType="centerCrop"
                android:src="@drawable/resis">

            </ImageView>
        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/waterbutton1555"
        android:layout_width="123dp"
        android:layout_height="127dp"
        android:layout_gravity="left"
        android:visibility="invisible"
        android:layout_marginStart="216dp"
        android:layout_marginTop="227dp"
        android:layout_marginBottom="302dp"
        app:cardCornerRadius="21dp"
        app:cardElevation="21dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/card_background"
            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dp"
                android:scaleType="centerCrop"
                android:src="@drawable/body">

            </ImageView>
        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/waterbutton155"
        android:visibility="invisible"
        android:layout_width="123dp"
        android:layout_height="127dp"
        android:layout_gravity="left"
        android:layout_marginTop="227dp"
        android:layout_marginEnd="216dp"
        android:layout_marginBottom="302dp"
        app:cardCornerRadius="21dp"
        app:cardElevation="21dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/card_background"
            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dp"
                android:scaleType="centerCrop"
                android:src="@drawable/bellyfat">

            </ImageView>
        </LinearLayout>

    </androidx.cardview.widget.CardView>





</androidx.constraintlayout.widget.ConstraintLayout>